{
  "name": "@nl-design-system-candidate/storybook",
  "author": "Community for NL Design System",
  "description": "No Storybook based on the NL Design System architecture",
  "license": "EUPL-1.2",
  "type": "module",
  "keywords": [
    "nl-design-system"
  ],
  "private": true,
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/candidate.git",
    "directory": "packages/no-storybook"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "build": "rimraf dist && vite build && cp dist/assets/*.js dist/index.js",
    "storybook": "vite",
    "dev": "vite",
    "lint": "tsc --noEmit",
    "todo:test": "vitest --coverage"
  },
  "devDependencies": {
    "@nl-design-system-candidate/button-css": "workspace:*",
    "@nl-design-system-candidate/button-docs": "workspace:*",
    "@nl-design-system-candidate/button-tokens": "workspace:*",
    "@nl-design-system-candidate/code-block-css": "workspace:*",
    "@nl-design-system-candidate/code-css": "workspace:*",
    "@nl-design-system-candidate/heading-css": "workspace:*",
    "@nl-design-system-candidate/paragraph-css": "workspace:*",
    "@nl-design-system-unstable/voorbeeld-design-tokens": "7.4.0",
    "@nl-design-system/tsconfig": "1.0.5",
    "@vitest/browser-playwright": "4.0.12",
    "@vitest/coverage-v8": "4.0.12",
    "playwright": "1.56.1",
    "rimraf": "6.1.2",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "vite": "7.1.12",
    "vitest": "4.0.12"
  },
  "dependencies": {
    "lit": "3.3.1",
    "preact": "10.28.0",
    "prettier": "3.6.2",
    "prismjs": "1.30.0"
  }
