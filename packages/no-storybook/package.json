{
  "name": "@nl-design-system-candidate/no-storybook",
  "author": "Community for NL Design System",
  "description": "No Storybook based on the NL Design System architecture",
  "license": "EUPL-1.2",
  "type": "module",
  "keywords": [
    "nl-design-system"
  ],
  "private": true,
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/candidate.git",
    "directory": "packages/no-storybook"
  },
  "scripts": {
    "start": "http-server dist/",
    "clean": "rimraf dist/ public/",
    "build": "pnpm run clean && pnpm run build:stories && vite build && cp dist/assets/*.js dist/index.js",
    "build:stories": "rimraf public/ && tsc --project tsconfig.stories.json",
    "storybook": "vite",
    "dev": "vite",
    "lint": "tsc --noEmit",
    "todo:test": "vitest --coverage"
  },
